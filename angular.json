{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "c44bbe3e-fac0-4385-b980-ea4ec7ae7a29"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@agencycoda/mia-finder": {
      "projectType": "library",
      "root": "projects/agencycoda/mia-finder",
      "sourceRoot": "projects/agencycoda/mia-finder/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/agencycoda/mia-finder/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/agencycoda/mia-finder/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/agencycoda/mia-finder/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/agencycoda/mia-finder/src/test.ts",
            "tsConfig": "projects/agencycoda/mia-finder/tsconfig.spec.json",
            "karmaConfig": "projects/agencycoda/mia-finder/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "@agencycoda/mia-finder"
}
